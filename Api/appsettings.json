{
  "ApplicationName": "elegant-databaser",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQLConnection": "Server=localhost;Port=5432;Database=MultiOperation;Username=postgres;Password=postgre;Integrated Security=true;Pooling=true;",
    "PostgreSQLServerConnection": "User ID=postgres;Password=postgre;Server=db_server;Port=5432;Database=MultiOperation;Pooling=true;",
    "RedisConnection": "localhost:6379",
    "ElasticConnection": "http://localhost:9200"
  },
  "Redis": {
    "DefaultExpirationTimeInMinutes": "5"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
